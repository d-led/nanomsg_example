.NOTPARALLEL:

ifndef config
  config=debug_x32
endif

ifndef verbose
  SILENT = @
endif

ifeq ($(config),debug_x32)
  cppnanomsg_check_config = debug_x32
endif
ifeq ($(config),debug_x64)
  cppnanomsg_check_config = debug_x64
endif
ifeq ($(config),release_x32)
  cppnanomsg_check_config = release_x32
endif
ifeq ($(config),release_x64)
  cppnanomsg_check_config = release_x64
endif

PROJECTS := cppnanomsg_check

.PHONY: all clean help $(PROJECTS) 

all: $(PROJECTS)

cppnanomsg_check:
ifneq (,$(cppnanomsg_check_config))
	@echo "==== Building cppnanomsg_check ($(cppnanomsg_check_config)) ===="
	@${MAKE} --no-print-directory -C . -f cppnanomsg_check.make config=$(cppnanomsg_check_config)
endif

clean:
	@${MAKE} --no-print-directory -C . -f cppnanomsg_check.make clean

